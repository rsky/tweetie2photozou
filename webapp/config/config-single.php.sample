 */
define('T2P_SINGLE_USERNAME', 'photozou_username');

/**
 * フォト蔵のパスワード
 *
 * @const string
 */
define('T2P_SINGLE_PASSWORD', 'photozou_password');

/**
 * アップロードするアルバムのID
 *
 * @const int
 */
define('T2P_SINGLE_ALBUM_ID', 0);

